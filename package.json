{
  "name": "discord-hybrid-music-bot",
  "version": "1.0.0",
  "description": "A hybrid Discord music bot with TypeScript, DisTube, slash commands, deploy on Railway",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "deploy-commands": "ts-node src/utils/deploy-commands.ts"
  },
  "keywords": ["discord", "music", "typescript", "disTube"],
  "author": "disa12311",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.5.0",
    "@discordjs/rest": "^1.5.0",
    "discord-api-types": "^0.37.0",
    "discord.js": "^14.0.0",
    "dotenv": "^16.0.0",
    "distube": "^4.0.0",
    "@distube/spotify": "^4.0.0",
    "@distube/soundcloud": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.0"
  }
}
